{
  "dashboard": {
    "title": "Macro Cycle HMM Model Monitoring",
    "tags": ["hmm", "macro", "ml"],
    "timezone": "browser",
    "schemaVersion": 27,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Prediction Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hmm_predictions_total[5m])",
            "legendFormat": "{{model_name}} - Regime {{regime}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Prediction Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(hmm_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "95th percentile - {{model_name}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(hmm_prediction_latency_seconds_bucket[5m]))",
            "legendFormat": "50th percentile - {{model_name}}",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Model Performance Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "hmm_model_log_likelihood",
            "legendFormat": "Log Likelihood - {{model_name}}",
            "refId": "A"
          },
          {
            "expr": "hmm_model_aic",
            "legendFormat": "AIC - {{model_name}}",
            "refId": "B"
          },
          {
            "expr": "hmm_model_bic",
            "legendFormat": "BIC - {{model_name}}",
            "refId": "C"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Regime Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "hmm_regime_distribution",
            "legendFormat": "Regime {{regime}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Prediction Errors",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(hmm_prediction_errors_total[5m])",
            "legendFormat": "{{error_type}} - {{model_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Feature Count",
        "type": "stat",
        "targets": [
          {
            "expr": "hmm_feature_count",
            "legendFormat": "{{model_name}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 16}
      }
    ]
  }
}

