{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "EDGAR - Download All Companies (10-K)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--filings",
                "--output-dir",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings",
                "--form-type",
                "10-K"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Download All Companies (10-Q)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--filings",
                "--output-dir",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings",
                "--form-type",
                "10-Q"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Download by Company Name (WMT, 10-K)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--filings",
                "--output-dir",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings",
                "--company-name",
                "Walmart",
                "--form-type",
                "10-K"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Process ZIP Files",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--process-zips",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Generate Catalog (All Companies)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--generate-catalog"
            ],
            "env": {
                "POSTGRES_PASSWORD": "2014",
                "POSTGRES_PORT": "5432"
            },
            "cwd": "${workspaceFolder}"
        }
    ]
}
