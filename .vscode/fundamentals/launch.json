            "args": [
                "--process-zips",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Generate Catalog (All Companies)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--generate-catalog"
            ],
            "env": {
                "POSTGRES_PASSWORD": "2014",
                "POSTGRES_PORT": "5432"
