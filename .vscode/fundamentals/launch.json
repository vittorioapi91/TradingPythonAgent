{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "EDGAR - Download All Companies (10-K, 10-Q)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--from-db",
                "--output-dir",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings",
                "--filing-types",
                "10-K",
                "10-Q"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Download Single Ticker (10-K, 10-Q)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/trading_agent/fundamentals/edgar.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--ticker",
                "WMT",
                "--output-dir",
                "${workspaceFolder}/src/trading_agent/fundamentals/edgar_filings",
                "--filing-types",
                "10-K",
                "10-Q"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "EDGAR - Download Multiple Tickers (10-K, 10-Q)",
